{
    "name": "astrogoat/elevar",
    "description": "A Elevar app for Strata",
    "keywords": [
        "astrogoat",
        "laravel",
        "elevar"
    ],
    "homepage": "https://github.com/astrogoat/elevar",
    "license": "MIT",
    "authors": [
        {
            "name": "OniiCoder",
            "email": "oniicoder@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "astrogoat/strata": "^0.3.0|^0.4.0",
        "astrogoat/shopify": "^1.48",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37 || ^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Astrogoat\\Elevar\\": "src",
            "Astrogoat\\Elevar\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrogoat\\Elevar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Astrogoat\\Elevar\\ElevarServiceProvider"
            ],
            "aliases": {
                "Elevar": "Astrogoat\\Elevar\\ElevarFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
